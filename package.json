{
  "name": "aws-spot-price",
  "version": "0.3.0",
  "description": "CLI utility to list current global AWS EC2 Spot Instance prices.",
  "keywords": [
    "aws",
    "ec2",
    "spot",
    "instance",
    "price",
    "prices",
    "pricing"
  ],
  "bugs": {
    "url": "https://github.com/hoonoh/aws-spot-price/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoonoh/aws-spot-price"
  },
  "author": {
    "name": "hoonoh"
  },
  "bin": "bin/aws-spot-price",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build",
    "postinstall": "patch-package",
    "clean": "rm -rf dist && rm -rf coverage",
    "build:ec2-types": "ts-node -T util/generate-ec2-types.ts",
    "build": "tsc",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "jest --runInBand --verbose",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test:coverage --ci --reporters=jest-junit --coverageReporters lcov --coverageReporters cobertura",
    "lint": "eslint **/*.ts",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "eslintIgnore": [
    "dist"
  ],
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "snapshotResolver": "./test/snapshot-resolver.js",
    "setupFilesAfterEnv": [
      "./test/jest.setup.js"
    ]
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogTitle": "# aws-spot-price changelog"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(next): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "publishConfig": {
    "tag": "next"
  },
  "dependencies": {
    "aws-sdk": "^2.544.0",
    "ora": "^4.0.2",
    "patch-package": "^6.2.0",
    "prompts": "^2.2.1",
    "table": "^5.4.6",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.5.4",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.7.5",
    "@types/prettier": "^1.18.3",
    "@types/prompts": "^2.0.1",
    "@types/table": "^4.0.7",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.25",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-typescript": "^6.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "jest-mock-console": "^1.0.0",
    "lodash": "^4.17.15",
    "nock": "^11.4.0",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.27",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
