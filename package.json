{
  "name": "aws-spot-price",
  "version": "1.0.22-prerelease.1",
  "description": "CLI utility to list current global AWS EC2 Spot Instance prices.",
  "keywords": [
    "aws",
    "ec2",
    "spot",
    "instance",
    "price",
    "prices",
    "pricing"
  ],
  "bugs": {
    "url": "https://github.com/hoonoh/aws-spot-price/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hoonoh/aws-spot-price"
  },
  "author": {
    "name": "hoonoh"
  },
  "packageManager": "yarn@3.1.1",
  "bin": "bin/aws-spot-price",
  "main": "dist/module.js",
  "types": "dist/module.d.ts",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build",
    "prepack": "yarn build",
    "prepare": "husky install",
    "clean": "rm -rf coverage",
    "build:ec2-info": "ts-node -T scripts/generate-ec2-info.ts",
    "build:ec2-types": "ts-node -T scripts/generate-ec2-types.ts",
    "build:regions": "ts-node -T scripts/generate-regions.ts",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "test": "jest --runInBand --verbose",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn test:coverage --reporters=default jest-junit --coverageReporters lcov --coverageReporters cobertura",
    "lint": "eslint \"**/*.ts\"",
    "types": "tsc",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "3.43.0",
    "@aws-sdk/client-sts": "3.43.0",
    "ora": "5.4.1",
    "prompts": "2.4.2",
    "table": "6.7.3",
    "yargs": "17.2.1"
  },
  "devDependencies": {
    "@aws-sdk/node-config-provider": "3.40.0",
    "@aws-sdk/types": "3.40.0",
    "@commitlint/cli": "15.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "27.0.3",
    "@types/lodash": "4.14.177",
    "@types/node": "16.11.10",
    "@types/prettier": "2.4.2",
    "@types/prompts": "2.0.14",
    "@types/yargs": "17.0.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "aws-sdk-client-mock": "0.5.6",
    "commitizen": "4.2.4",
    "conventional-changelog-cli": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.3.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-junit": "13.0.0",
    "jest-mock-console": "1.2.3",
    "lodash": "4.17.21",
    "nock": "13.2.1",
    "prettier": "2.5.0",
    "semantic-release": "18.0.1",
    "supports-color": "9.2.1",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.2",
    "yargs-parser": "21.0.0"
  }
}
